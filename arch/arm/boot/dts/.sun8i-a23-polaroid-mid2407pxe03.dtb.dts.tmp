   };

   i2c2_pins_a: i2c2@0 {
    allwinner,pins = "PE12", "PE13";
    allwinner,function = "i2c2";
    allwinner,drive = <0>;
    allwinner,pull = <0>;
   };

   lcd_rgb666_pins: lcd-rgb666@0 {
    allwinner,pins = "PD2", "PD3", "PD4", "PD5", "PD6", "PD7",
