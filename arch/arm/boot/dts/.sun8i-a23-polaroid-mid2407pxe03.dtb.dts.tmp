   };

   mmc1_pins_a: mmc1@0 {
    allwinner,pins = "PG0", "PG1", "PG2",
       "PG3", "PG4", "PG5";
    allwinner,function = "mmc1";
    allwinner,drive = <2>;
    allwinner,pull = <0>;
   };

   mmc2_8bit_pins: mmc2_8bit {
    allwinner,pins = "PC5", "PC6", "PC8",
